{
    "name":  "%pipelineName",
    "project":  "%projectName",
    "triggers": [
            {
                "branchFilters": [],
                "pathFilters": [],
                "settingsSourceType": 2,
                "batchChanges": false,
                "maxConcurrentBuildsPerBranch": 1,
                "triggerType": "continuousIntegration"
            }
        ],
    "variables": {
        "varEnvironment": {
            "value": "dev,int,uat,prod",
            "allowOverride": true
        },
        "varRepoName": {
            "value": "my_test_api",
            "allowOverride": true
        },
        "varApplicationDeployType": {
            "value": "app_service",
            "allowOverride": true
        },
        "varArtifactFeedName": {
            "value": "none",
            "allowOverride": true
        },
        "varApplicationBuildPath": {
            "value": "$(glbDevSecOpsRepoName)/",
            "allowOverride": true
        }
    },
    "repository":  {
                       "url":  "%repoUri",
                       "defaultBranch":  "refs/heads/main",
                       "type":  "TfsGit",
                       "id":  "%repositoryId"
                   },
    "process":  {
                    "yamlFilename":  "%outputNameFile",
                    "type":  2
                },
    "path":  "\\CI_CD_Master_Pipelines",
    "type":  "build"
}